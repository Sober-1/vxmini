.titles{
  text-align: center;
  display:flex;
}
.left{
  display: flex;
  flex-direction: column;
}
.item{
  height: 120rpx;
  width: 260rpx;
  line-height: 120rpx;
  background-color: #f6f6f6;
  color: #666;
}
.active{
  color: #ff5777;
  border-left: 2px solid #ff5777;
}
.right{
  display: flex;
  flex-wrap: wrap;
}
.img{
  width: 150rpx;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  margin-left: 30rpx;
  margin-top: 20rpx;
}
.right image{
  width: 150rpx;
  height: 150rpx;
}
.img .text{
  text-align: center;
  width: 100%;
  font-size: 24rpx;
  color: #666;
  margin-top: 20rpx;
}

